{
    "cniVersion": "0.4.0",
    "name": "bridge",
    "type": "bridge",
    "bridge": "cnio0",
    "isGateway": true,
    "ipMasq": true,
    "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": "{{ '.'.join(POD_CIDR.split("/")[0].split('.')[:-2]) }}.{{ HOST_IP[inventory_hostname].split(".")[-1] }}.0/24]
        ],
        "routes": [{"dst": "0.0.0.0/0"}]
    }
}