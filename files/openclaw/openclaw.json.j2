{
  "agents": {
    "defaults": {
      "model": {
        "primary": "anthropic/claude-sonnet-4-20250514",
        "fallbacks": [
          "openai/gpt-4o",
          "google/gemini-2.0-flash",
          "llamacpp/auto"
        ]
      },
      "memorySearch": {
        "enabled": true,
        "provider": "openai"
      }
    },
    "list": [
      {
        "id": "boss",
        "default": true,
        "name": "Boss",
        "identity": {
          "name": "Boss",
          "emoji": "üèóÔ∏è"
        },
        "subagents": {
          "allowAgents": ["*"],
          "model": {
            "primary": "openai/gpt-4o",
            "fallbacks": ["google/gemini-2.0-flash", "llamacpp/auto"]
          }
        }
      }
    ]
  },
  "models": {
    "providers": {
      "llamacpp": {
        "baseUrl": "http://192.168.1.143:8080/v1",
        "apiKey": "{{ ai_services.llamacpp.api_key | default('') }}",
        "api": "openai-completions",
        "models": [
          {
            "id": "auto",
            "name": "Local LLM (llamacpp)",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 40960,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "port": 18789,
    "auth": {
      "mode": "token",
      "token": "{{ ai_services.openclaw.gateway_token | default('changeme-generate-a-real-token') }}"
    },
    "controlUi": {
      "allowInsecureAuth": true,
      "dangerouslyDisableDeviceAuth": true
    },
    "trustedProxies": ["192.168.1.143"]
  },
  "browser": {
    "enabled": true,
    "headless": true,
    "noSandbox": true,
    "executablePath": "/usr/bin/chromium"
  },
  "channels": {
    "whatsapp": {
      "dmPolicy": "pairing"
    },
    "telegram": {
      "enabled": true,
      "botToken": "{{ ai_services.openclaw.telegram_bot_token | default('') }}",
      "dmPolicy": "pairing",
      "streamMode": "partial"
    },
    "discord": {
      "enabled": true,
      "token": "{{ ai_services.openclaw.discord_bot_token | default('') }}",
      "dmPolicy": "pairing",
      "groupPolicy": "open"
    },
    "slack": {
      "enabled": true
    },
    "signal": {
      "enabled": false
    }
  }
}
